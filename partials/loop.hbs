{{#foreach posts}}

<article class="{{post_class}}">
	<div class="inner">
		<div class="box post-box">
			<center><h3 class="post-title"><a href="{{url}}">{{{title}}}</a></h3></center>
			<section class="post-content">
				{{content}}
			</section>
		</div>
	</div>
</article>

{{/foreach}}

{{pagination}}
